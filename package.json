  "description": "SQL Toolbox - Model Implementer, Execution Plan Visualizer and Query Comparison Tool",
  "main": "./tsCompiled/backend/main.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
